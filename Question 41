#include <iostream>
using namespace std;
int main() {
    int units;
    cout<<"Enter the units consumed: ";
    cin>>units;
    if(units<200){
        cout<<"No shedding";
    }
    else if(units>=200&&units<=500){
        cout<<"Load shedding time: 2 hours";
    }
    else{
        cout<<"Load shedding time: 5 hours";
    }
    return 0;
}
