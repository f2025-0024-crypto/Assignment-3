#include <iostream>
#include <cmath>
using namespace std;

int main() {
    int start, end;
    cout << "Enter starting value: ";
    cin >> start;
    cout << "Enter ending value: ";
    cin >> end;

    cout << "\nArmstrong numbers between " << start << " and " << end << " are:\n";

    for (int num = start; num <= end; num++) {
        int temp = num;
        int digits = 0;

        // Count number of digits
        while (temp > 0) {
            digits++;
            temp /= 10;
        }

        temp = num;
        int sum = 0;

        // Calculate sum of digits raised to power 'digits'
        while (temp > 0) {
            int digit = temp % 10;
            sum += pow(digit, digits);
            temp /= 10;
        }

        // Check Armstrong condition
        if (sum == num) {
            cout << num << " ";
        }
    }

    return 0;
}
