#include <iostream>
using namespace std;

long long power(int a, int b) {
    long long result = 1;
    for (int i = 1; i <= b; i++) {
        result *= a;
    }
    return result;
}

int main() {
    int a, b;
    cout << "Enter a and b: ";
    cin >> a >> b;

    cout << a << "^" << b << " = " << power(a, b);
}
