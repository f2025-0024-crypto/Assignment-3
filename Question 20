#include <iostream>
using namespace std;

int main() {
    long long n;
    cout << "Enter a number: ";
    cin >> n;

    // If number is negative, make it positive
    if (n < 0) n = -n;

    long long temp = n;
    bool hasZero = false;

    // Check last digits for any zero
    while (temp > 0) {
        if (temp % 10 == 0)
            hasZero = true;
        temp /= 10;
    }

    // Check if number begins with zero â€” not needed because
    // numeric input cannot start with zero unless the number is 0.
    // But we still handle the case:
    bool startsWithZero = false;
    if (n != 0) {
        long long t = n;
        while (t >= 10)
            t /= 10;
        if (t == 0)
            startsWithZero = true;
    }

    if (hasZero && !startsWithZero)
        cout << n << " is a Duck Number." << endl;
    else
        cout << n << " is NOT a Duck Number." << endl;

    return 0;
}
